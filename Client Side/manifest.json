{
  "manifest_version": 2,

  "name": "Google Vocab",
  "description": "This will save the search result of vocabulary words with meaning.",
  "version": "1.0",

  "permissions": [
    "http://sarcnitj.com/Server%20Side/",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
